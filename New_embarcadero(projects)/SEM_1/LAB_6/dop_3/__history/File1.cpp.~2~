int _tmain(int argc, _TCHAR* argv[])
{  int array_of_factorials [20];
   string s;
	 while (1)
	 { int kolvo,y;
	 cin>>s;

	 //y=y_function(s);
	 y=y_counter(s);
kolvo = factorial(array_of_factorials, s.size()) / factorial(array_of_factorials,y);
	   cout<<kolvo;
	 }

   getch();	return 0;
}
long long factorial (int array_of_factorials [], int M ){
 array_of_factorials[0]=1;
 for (int i = 1; i <= 20; i++)
	{array_of_factorials[i]=array_of_factorials[i-1]*i;


	}

 return array_of_factorials[M];
 }
